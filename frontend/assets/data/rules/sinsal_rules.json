  "rules": [
    {
      "id": "cheon_eul_gwin",
      "name": "천을귀인",
      "hanja": "天乙貴人",
      "type": "sinsal",
      "category": "특수신살",
      "fortuneType": "길",
      "when": {
        "op": "or",
        "conditions": [
          {
            "op": "and",
            "conditions": [
              { "field": "dayGan", "op": "in", "value": ["갑", "무", "경"] },
              { "field": "jiAny", "op": "in", "value": ["축", "미"] }
            ]
          },
          {
            "op": "and",
            "conditions": [
              { "field": "dayGan", "op": "in", "value": ["을", "기"] },
              { "field": "jiAny", "op": "in", "value": ["자", "신"] }
            ]
          },
          {
            "op": "and",
            "conditions": [
              { "field": "dayGan", "op": "in", "value": ["병", "정"] },
              { "field": "jiAny", "op": "in", "value": ["해", "유"] }
            ]
          },
          {
            "op": "and",
            "conditions": [
              { "field": "dayGan", "op": "eq", "value": "신" },
              { "field": "jiAny", "op": "in", "value": ["인", "오"] }
            ]
          },
          {
            "op": "and",
            "conditions": [
              { "field": "dayGan", "op": "in", "value": ["임", "계"] },
              { "field": "jiAny", "op": "in", "value": ["묘", "사"] }
            ]
          }
        ]
      },
      "reasonTemplate": "일간 {dayGan}에서 {matchedJi}가 천을귀인",
      "description": "귀인의 도움을 받는 길성입니다. 어려운 일이 있어도 도움을 받아 해결됩니다.",
      "priority": 100,
      "enabled": true
    },
